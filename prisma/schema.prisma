generator client {
  provider = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}


model Jugadores{
  id           Int    @id @default(autoincrement())
  nombre       String
  apellido      String
  edad         Int
  altura        Float
  peso         Float
  posicion     String
}
